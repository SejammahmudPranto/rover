Servo servo2;
Servo servo3;

const int angle;
void setup() {
  servo1.attach(3);
  servo2.attach(7);
  servo3.attach(11);
  delay(1000);
  
  // put your setup code here, to run once:

}

void loop() {
  int i,j;
  for(i=50; i<=80; i++) {
  servo2.write(i);
  delay(50);
 }
 for(i=80; i>=50; i--) {
  servo2.write(i);
  delay(50);
 }
