void forward() {      //car forward
  rfmfor();
  rmmfor();
  rbmfor();
  lfmfor();
  lmmfor();
  lbmfor();
}

void reverse() {      //car reverse
  rfmrev();
  rmmrev();
  rbmrev();
  lfmrev();
  lmmrev();
  lbmrev();
}

